    "from gtts import gTTS\n",
    "import os\n",
    "\n",
    "text=input(\"Enter the Text\")\n",
    "language = 'en'\n",
    "\n",
    "speech = gTTS(text=text, lang=language, slow=True)\n",
    "speech.save(\"output.mp3\")\n",
    "\n",
    "os.system(\"start output.mp3\") "
   ]
